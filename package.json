{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@gsap/react": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "gsap": "^3.12.5",
    "libphonenumber-js": "^1.11.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.22.3",
    "react-transition-group": "^4.4.5",
    "serve": "^14.2.3",
    "terser": "^5.31.0",
    "validator": "^13.12.0",
    "vite-plugin-env-compatible": "^2.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development vite",
    "start:app": "cross-env NODE_ENV=development VITE_APP_VERSION=long vite",
    "start:short": "cross-env NODE_ENV=development VITE_APP_VERSION=short vite",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview --port 8080",
    "minify": "terser public/logger/lg_logger.js -o public/logger/lg_logger.min.js -c -m",
    "build:minify": "npm run minify && vite build",
    "build:app": "cross-env NODE_ENV=production VITE_BASE_URL=/application/ VITE_APP_VERSION=long vite build",
    "build:short": "cross-env NODE_ENV=production VITE_BASE_URL=/application-short/ VITE_APP_VERSION=short vite build",
    "preview:app": "cross-env NODE_ENV=production VITE_BASE_URL=/application/ vite preview --port 8080",
    "preview:short": "cross-env NODE_ENV=production VITE_BASE_URL=/application-short/ vite preview --port 8080"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "dotenv": "^16.4.5",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0"
  }
}
